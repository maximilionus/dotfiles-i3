#!/usr/bin/env bash

WMENU_ARGS=(-f "Sans-serif 14" -i -N 080808 -n ffffff -S ffffff -s 000000 -M c8c8c8 -m 000000)
BIN_NAME=$(basename $0)


if [[ "$BIN_NAME" != "wmenu" && "$BIN_NAME" != "wmenu-run" ]]; then
    echo "Wrong basename"
    exit 1
fi

exec /usr/bin/"$BIN_NAME" "${WMENU_ARGS[@]}" "${@}"
